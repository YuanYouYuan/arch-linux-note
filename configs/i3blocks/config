command=/usr/lib/i3blocks/$BLOCK_NAME
separator_block_width=15
markup=none

[mpd]
command=$HOME/.config/i3blocks/i3mpd.sh
interval=3
align=left

[memory]
command=$HOME/.config/i3blocks/i3memory.sh
interval=10
separator=true
markup=pango

[volume]
command=$HOME/.config/i3blocks/i3volume.sh
interval=1
separator=true
markup=pango

[light]
command=$HOME/.config/i3blocks/i3light.sh
interval=1
separator=true
markup=pango

[battery]
command=$HOME/.config/i3blocks/i3battery.sh
interval=1
separator=true
markup=pango

[time]
command=$HOME/.config/i3blocks/i3time.sh
interval=1
separator=true
markup=pango
