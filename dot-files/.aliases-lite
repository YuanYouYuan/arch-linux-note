# vim:filetype=zsh

alias tm='tmux attach-session || tmux'
alias ez='exec zsh'
alias p='python'
alias p2='python2'
alias ipy='ipython'
alias v='vim'
alias rg='ranger --choosedir=$HOME/.rangerdir; LASTDIR=`cat $HOME/.rangerdir`; cd "$LASTDIR"'
alias h='history'


## cd/navigation
alias '..'='cd ../'
alias '...'=../..
alias '....'=../../..
alias '.....'=../../../..
alias '......'=../../../../..
alias 1='cd -'
alias 2='cd -2'
alias 3='cd -3'
alias 4='cd -4'
alias 5='cd -5'
alias 6='cd -6'
alias 7='cd -7'
alias 8='cd -8'
alias 9='cd -9'

# dirs
alias d='dirs -v | head -10'
alias po=popd
alias pu=pushd

## ls/list
alias l='ls -lah'
alias la='ls -lAh'
alias ll='ls -lh'
alias lt='ls -rlath'
alias ls='ls --color=tty'
alias lsa='ls -lah'
